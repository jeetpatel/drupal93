{#
/**
 * @file learning_entity.html.twig
 * Default theme implementation to present Learning entity data.
 *
 * This template is used when viewing Learning entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_learning_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('learning_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
