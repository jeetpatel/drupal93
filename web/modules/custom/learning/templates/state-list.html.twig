{% include '@learning/learning--template.html.twig' with {source_name: 'state--list.html.twig'} %}

{% set state_class = 'state list'|clean_class %}
<div class="state_list_wrapper">
<h2>{% trans %} Custom Message:: {{- custom_message -}} {% endtrans %}</h2>
<h2>State List:</h2>
<p>
state list|clean_class = {{ state_class }}
</p>
<ul>
{% for state_name in state_data %}
  <li>{{ state_name }}</li>
{% endfor %}
</ul>
</div>